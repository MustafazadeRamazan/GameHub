
@{ ViewBag.Title = "Home"; }

<!-- Start slider -->
<section id="aa-slider">
    <div class="aa-slider-area">
        <div id="sequence" class="seq">
            <div class="seq-screen">
                <ul class="seq-canvas">

                    <!-- single slide item -->
                    @foreach (var item in @ViewBag.Slider)
                    {
        <li>
            <div class="seq-model">
                <img data-seq src="@item.ImageURL" alt="@item.AltText" />
            </div>
            <div class="seq-title">
                <span data-seq>@item.OfferTag</span>
                <h2 data-seq>@item.Title</h2>
                <p data-seq>@item.Description</p>
                <a data-seq href="@Url.Action("GetProductsByCategory", "Product", new { categoryName = "Fps Games" })" class="aa-shop-now-btn aa-secondary-btn">@item.BtnText</a>
            </div>
        </li>}
                    <!-- end single slide item -->

                </ul>
            </div>
            <!-- slider navigation btn -->
            <fieldset class="seq-nav" aria-controls="sequence" aria-label="Slider buttons">
                <a type="button" href="" class="seq-prev" aria-label="Previous"><span class="fa fa-angle-left"></span></a>
                <a type="button" href="" class="seq-next" aria-label="Next"><span class="fa fa-angle-right"></span></a>
            </fieldset>
        </div>
    </div>
</section>
<!-- / slider -->
<!-- Start Promo section -->
<section id="aa-promo">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-promo-area">
                    <div class="row">
                        <!-- promo left -->
                        <div class="col-md-5 no-padding">
                            <div class="aa-promo-left">
                                <div class="aa-promo-banner">
                                    <img src="~/Content/img/promo-banner-gameaccount.jpg" alt="img">
                                    <div class="aa-prom-content">
                                        <span>75% Off</span>
                                        <h4><a href="@Url.Action("GetProductsByCategory", "Product", new { @categoryName = "Fps Games" })">Fps Games</a></h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- promo right -->
                        <div class="col-md-7 no-padding">
                            <div class="aa-promo-right">
                                @foreach (var item in @ViewBag.PromoRight)
                                {

                    <div class="aa-single-promo-right">
                        <div class="aa-promo-banner">
                            <img src="@item.ImageURL" alt="@item.AltText">
                            <div class="aa-prom-content">
                                <span>@item.OfferTag</span>
                                <h4><a href="@Url.Action("GetProductsByCategory", "Product", new { @categoryName = @item.Category.Name })">@item.Title</a></h4>
                            </div>
                        </div>
                    </div>}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Promo section -->
<!-- Products section -->
<section id="aa-product">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="aa-product-area">
                        <div class="aa-product-inner">
                            <!-- start prduct navigation -->
                            <ul class="nav nav-tabs aa-products-tab">
                                <li class="active"><a href="#fps" data-toggle="tab">Fps Games</a></li>
                                <li><a href="#gp" data-toggle="tab">Game Projects</a></li>
                                <li><a href="#sports" data-toggle="tab">Sport</a></li>
                                <li><a href="#aa" data-toggle="tab">Action Adventure</a></li>
                                <li><a href="#sg" data-toggle="tab">Survival Games</a></li>
                            </ul>
                            <!-- Tab panes -->
                            <div class="tab-content">
                                <!-- Start FpsGames product category -->
                                <div class="tab-pane fade in active" id="fps">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @foreach (var item in (List<GameHub.Models.Product>)ViewBag.FpsGames)
                                        {
                                            if (item.UnitInStock == 0)
                                            {
                                                item.AddBadge = true;
                                                item.OfferBadgeClass = "aa-sold-out";
                                                item.OfferTitle = "SOLD OUT";
                                            }
                                            { Html.RenderPartial("_Product", item); }
                                        }
                                        <!-- end start single product item -->

                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("GetProductsByCategory", "Product", new { categoryName = "Fps Games" })">Browse all Account <span class="fa fa-long-arrow-right"></span></a>
                                </div>

                                <!-- start women product category -->
